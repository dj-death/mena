                                <table class="table table-hover text-center table-bordered">
                                    <tbody class="table-striped">
                                    <tr class="bg-gray">
                                        <!--<th>ID</th>-->
                                        <th>Identifiant</th>
                                        <th>E-mail</th>
                                        <th>Statut</th>
                                        <th>Jetons utilisés</th>
                                        <th>Tél.</th>
                                        <th>Pays / Ville</th>
                                        <th>Adresse</th>
                                        <!--<th>ID Carte</th>-->
                                    </tr>
                                    <tr ng-repeat="user in data.distributors" >
                                        <!--<td>{{user._id}}</td>-->
                                        <td>{{user.username}}</td>
                                        <td>{{user.email}}</td>
                                        <td>{{user.activated}}</td>
                                        <td>{{user.numOfUsedLicense}}/{{user.numOfLicense}}</td>
                                        <td>{{user.mobilePhone}}</td>
                                        <td>{{user.country}} / {{user.city}} </td>
                                        <td>{{user.address}} {{user.street}}</td>
                                        <!--<td>{{user.idcardNumber}}</td>-->
                                    </tr>
                                    </tbody>
                                </table>
                            </div><!-- /.box-body -->
                        </div>

                    </div>
                </div>
            </section>

            <section class="content" ng-if="css.leftmenu==22">
                <h2 class="page-header">Nouveau Distributeur</h2>

