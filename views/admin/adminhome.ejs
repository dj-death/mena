<% include ./include/headcssadmin.html %>


<body class="hold-transition fixed skin-blue" ng-app="marksimosadmin" ng-controller="adminHomeController">

    <header class="main-header">
		<header-admin currentuser="data.currentUser"></header-admin>
	</header>

    <div class="wrapper row-offcanvas row-offcanvas-left">

        <!-- Left side column. contains the logo and sidebar -->
        <menu-admin current-menu="css.leftmenu" showtab="css.menuTabShow" currentuser="data.currentUser" changemenu="changeMenu()"></menu-admin>


        <aside class="right-side" ng-cloak>

            <section class="content" ng-if="css.leftmenu==11">
                <h2 class="page-header">Page d'accueil</h2>

                <div class="row" >
                    <div class="col-md-12">

                        <div class="box box-primary">
                            <form class="form-horizontal" role="form">

                                <div class="form-group">
                                    <label class="col-xs-2 control-label  text-right">Etudiants inscrits total:</label>
                                    <div class="col-xs-5">
                                        <p class="form-control-static">{{data.studentsCount[2].count}}</p>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-xs-2 control-label  text-right">Etudiants inscrits jusqu'au ({{data.studentsCount[0].date}}):</label>
                                    <div class="col-xs-5">
                                        <p class="form-control-static">{{data.studentsCount[0].count}}</p>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-xs-2 control-label  text-right">Etudiants inscrits jusqu'au ({{data.studentsCount[1].date}}):</label>
                                    <div class="col-xs-5">
                                        <p class="form-control-static">{{data.studentsCount[1].count}}</p>
                                    </div>
                                </div>

                                <hr>

                                <div class="form-group" ng-repeat="campaign in data.campaigns">
                                    <label class="col-xs-2 control-label text-right">Campaign :</label>
                                    <div class="col-xs-2">
                                        <p class="form-control-static">{{campaign.name}}</p>
                                    </div>

                                    <label class="col-xs-2 control-label  text-right">Teams count:</label>
                                    <div class="col-xs-1">
                                        <p class="form-control-static">{{campaign.teamList.length}}</p>
                                    </div>

                                    <label class="col-xs-2 control-label  text-right">Members count:</label>
                                    <div class="col-xs-2">
                                        <p class="form-control-static">{{getTeamMemberCount(campaign.teamList)}}</p>
                                    </div>
                                </div>

                                <hr>

                                <div class="form-group">
                                    <label class="col-xs-2 control-label  text-right">New joined teams count({{data.teamCount[0].date}}):</label>
                                    <div class="col-xs-1">
                                        <p class="form-control-static">{{data.teamCount[0].count}}</p>
                                    </div>
                                    <label class="col-xs-2 control-label  text-right">New joined members count({{data.teamCount[0].date}}):</label>
                                    <div class="col-xs-1">
                                        <p class="form-control-static">{{data.teamCount[0].memberCount}}</p>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-xs-2 control-label  text-right">New joined teams count({{data.teamCount[1].date}}):</label>
                                    <div class="col-xs-1">
                                        <p class="form-control-static">{{data.teamCount[1].count}}</p>
                                    </div>
                                    <label class="col-xs-2 control-label  text-right">New joined members count({{data.teamCount[1].date}}):</label>
                                    <div class="col-xs-1">
                                        <p class="form-control-static">{{data.teamCount[1].memberCount}}</p>
                                    </div>
                                </div>

                            </form>
                        </div>
                    </div>
                </div>
            </section>


            <section class="content" ng-if="css.leftmenu==21">
                <h2 class="page-header">Gestion des Distributeurs</h2>

                <div class="row" >
                    <div class="col-md-12">

                        <div class="box box-primary">
                            <form class="form-horizontal" role="form" name="searchDistributorForm">
                                <div class="box-body">

                                    <text-form-input type="text" name="newDistributorUsername" label="Nom :" placeholder="Nom du distributeur" labelclass="col-sm-2" inputclass="col-sm-4" data="data.searchDistributor.username" minlength="3" minlengtherrorinfo="Username must be at least 3 characters" maxlength="20" maxlengtherrorinfo="Username is a maximum of 20 characters"></text-form-input>
                                    <text-form-input type="email" name="newDistributorEmail" label="E-mail :" placeholder="Email" labelclass="col-sm-2" inputclass="col-sm-4" data="data.searchDistributor.email" emailerrorinfo="Doesn't look like a valid email !" ></text-form-input>

                                    <div class="form-group">
                                        <label for="inputDistributorUserStatus" class="col-sm-2 control-label">Statut</label>
                                        <div class="col-sm-4">
                                            <label class="radio-inline">
                                                <input type="radio" name="inputDistributorUserStatus" ng-model="data.searchDistributor.user_status" value="true">Actif
                                            </label>
                                            <label class="radio-inline">
                                                <input type="radio" name="inputDistributorUserStatus" ng-model="data.searchDistributor.user_status" value="false">Inactif
                                            </label>
                                        </div>
                                    </div>

                                </div>

                                <div class="box-footer">
                                    <div class="form-group">
                                        <div class="col-sm-offset-2 col-sm-10">
                                            <button type="submit" class="btn btn-primary" ng-click="searchDistributor(searchDistributorForm)">Rechercher</button>
                                        </div>
                                    </div>
                                </div>

                            </form>
                        </div>

                        <div class="box">
                            <div class="box-header">
                                <h3 class="box-title">Liste des Distributeurs</h3>
                            </div><!-- /.box-header -->
