                                    <text-form-input type="email" name="newDistributorEmail" label="E-mail :" placeholder="" labelclass="col-sm-2" inputclass="col-sm-4" data="data.newDistributor.email" required="true" requirederrorinfo="Email is required!" emailerrorinfo="Doesn't look like a valid email !" ></text-form-input>
                                    <text-form-input type="text" name="newDistributorPassword" label="Mot de Passe :" placeholder="" labelclass="col-sm-2" inputclass="col-sm-4" data="data.newDistributor.password" required="true" requirederrorinfo="Password is required!" minlength="6" minlengtherrorinfo="Password must be at least 6 characters" maxlength="20" maxlengtherrorinfo="Password is a maximum of 20 characters" ></text-form-input>

