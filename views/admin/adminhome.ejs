                            </div><!-- /.box-header -->
                            <div class="box-body table-responsive no-padding">
                                <table class="table table-striped table-hover text-center table-bordered table-striped">
                                    <tbody class="table-striped">
                                    <tr class="bg-gray">
                                        <!-- <th>ID</th> -->
                                        <th><em class="fa fa-cog"></em> Action</th>
                                        <th>Identifiant</th>
                                        <th>Equipe</th>
                                        <th>Date d'inscr.</th>
                                        <th>Nom Complet</th>
                                        <th>Sexe</th>
                                        <th>Statut</th>
                                        <th>E-mail</th>
                                        <th>E-mail Activé ?</th>
                                        <th>Tél.</th>
                                        <th>Tél. vérifié ?</th>
                                        <th>Localisation</th>
                                        <th>Université</th>
                                        <th>Major</th>
                                        <th>Expériences prof.</th>
                                        <th>Rejoindre Campaign</th>
                                        <th>Rejoindre Campaign Name</th>
                                        <th>Rejoindre Campaign As Team Member</th>
                                        <th>Réinit. mot de passe</th>
                                    </tr>
                                    <tr ng-repeat="user in data.students" >
                                        <!-- <td>{{user._id}}</td> -->
                                        <td><button type="button" class="btn btn-danger" ng-click="data.currentStudent = user">View</button></td>
                                        <td>{{user.username}}</td>
                                        <td>{{user.team.name}}</td>
                                        <td>{{user.createdAt | date:'yyyy-MM-dd HH:mm:ss'}}</td>
                                        <td>{{user.firstName}} {{user.lastName}} </td>
                                        <td>{{user.gender == 1 ? 'ProfilePageBasicInfoGenderMale' : 'ProfilePageBasicInfoGenderFemale' | translate}} </td>
                                        <td>{{user.activated}}</td>
                                        <td>{{user.email}} </td>
                                        <td>{{user.emailActivated}}</td>
                                        <td>{{user.mobilePhone }} </td>
                                        <td>{{user.phoneVerified}}</td>
                                        <td>{{user.currentLocation}} </td>
                                        <td> <span ng-repeat="edu in user.eductionBackgrounds">{{edu.university}} <br/> </span> </td>
                                        <td> <span ng-repeat="edu in user.eductionBackgrounds">{{edu.major | major | translate}} <br/> </span> </td>
                                        <td> <span ng-repeat="work in user.workExperiences">{{work.company}}  <br/> </span> </td>
                                        <td>{{user.joinedCampaign.length}}</td>
                                        <td><span ng-repeat="campaign in user.joinedCampaign">{{campaign.name}}<br/></span></td>
