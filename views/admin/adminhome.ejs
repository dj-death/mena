                                        <div class="col-sm-3">
                                            <input type="text" class="form-control" id="inputDistributorAddress" placeholder="Adresse" ng-model="data.newDistributor.address">
                                        </div>
                                    </div>

                                    <text-form-input type="number" name="newDistributorLicences" label="Nb. Jetons attribués :" placeholder="" labelclass="col-sm-2" inputclass="col-sm-4" data="data.newDistributor.numOfLicense" required="true" requirederrorinfo="License No. is required!" min="1" minerrorinfo="Must be greater than 1" max="200" maxerrorinfo="Must be less than 200"></text-form-input>

                                </div>

                                <div class="box-footer">
                                    <div class="form-group">
                                        <div class="col-sm-offset-2 col-sm-10">
                                            <button type="submit" class="btn btn-primary" ng-click="createNewDistributor(newDistributorForm)"> Créer </button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>



                    </div>
                </div>
            </section>


