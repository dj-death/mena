<% include ./include/headcssadmin.html %>


<body class="hold-transition fixed skin-blue" ng-app="marksimosadmin" ng-controller="adminMarksimosReportController" ng-cloak>
    
	<header class="main-header">
		<header-admin class="header fixedtop" currentuser="data.currentUser"></header-admin>
	</header>

    <chat-window hide-chat-header="true" send-seminar-message="sendSeminarMessage(messageInput)" username="data.currentUser.username" seminar-messages="data.seminarChatMessages" ></chat-window>


    <div class="wrapper row-offcanvas row-offcanvas-left ">

        <section class="content fixedtop">

            <div class="row">
                <div class="col-md-12">
                    <h2 class="page-header" ng-class="{'menutitle':!css.showReportMenu}">MarkSimos Seminar <%= seminarId %> Report
                        <a class="btn btn-app iconbtn" ng-click="css.showReportMenu=!css.showReportMenu"> <i class="fa fa-sort-asc"></i> </a>
                    </h2>

                </div>
            </div>

            <div class="row" ng-if="css.showReportMenu">
                <div class="col-md-7">
                    {{'ReportYourCompany' | translate}} :
                    <div class="btn-group btn-group-xs">
                        <button type="button" class="btn btn-default" ng-class="{'active': css.currentReportMenu=='A1'}" ng-click="clickChartMenu('A1')">{{'ReportMenuCompanyStatus' | translate}}</button>
                        <button type="button" class="btn btn-default" ng-class="{'active': css.currentReportMenu=='A2'}" ng-click="clickChartMenu('A2')">{{'ReportMenuFinancialReport' | translate}}</button>
                        <button type="button" class="btn btn-default" ng-class="{'active': css.currentReportMenu=='A3'}" ng-click="clickChartMenu('A3')">{{'ReportMenuInventoryReport' | translate}}</button>
                        <button type="button" class="btn btn-default" ng-class="{'active': css.currentReportMenu=='A4'}" ng-click="clickChartMenu('A4')">{{'ReportMenuProfitabilityEvolution' | translate}}</button>
                    </div>
                    <br />

                    {{'ReportYourCompetitors' | translate}} :
                    <div class="btn-group btn-group-xs">
                        <button type="button" class="btn btn-default" ng-class="{'active': css.currentReportMenu=='B1'}" ng-click="clickChartMenu('B1')">{{'ReportMenuMarketShare' | translate}}</button>
                        <button type="button" class="btn btn-default" ng-class="{'active': css.currentReportMenu=='B2'}" ng-click="clickChartMenu('B2')">{{'ReportMenuCompetitorIntelligence' | translate}}</button>
                        <button type="button" class="btn btn-default" ng-class="{'active': css.currentReportMenu=='B3'}" ng-click="clickChartMenu('B3')">{{'ReportMenuInvestmentProfits' | translate}}</button>
                        <button type="button" class="btn btn-default" ng-class="{'active': css.currentReportMenu=='B4'}" ng-click="clickChartMenu('B4')">{{'ReportMenuMarketSalesInventory' | translate}}</button>
                    </div>
                    <br />

                    {{'ReportMarketLandscape' | translate}} :
                    <div class="btn-group btn-group-xs">
                        <button type="button" class="btn btn-default" ng-class="{'active': css.currentReportMenu=='C1'}" ng-click="clickChartMenu('C1')">{{'ReportMenuSegmentLeaderTop5' | translate}}</button>
                        <button type="button" class="btn btn-default" ng-class="{'active': css.currentReportMenu=='C2'}" ng-click="clickChartMenu('C2')">{{'ReportMenuPerceptionMap' | translate}}</button>
                        <button type="button" class="btn btn-default" ng-class="{'active': css.currentReportMenu=='C3'}" ng-click="clickChartMenu('C3')">{{'ReportMenuSegmentDistributions' | translate}}</button>
                        <button type="button" class="btn btn-default" ng-class="{'active': css.currentReportMenu=='C4'}" ng-click="clickChartMenu('C4')">{{'ReportMenuMarketEvolution' | translate}}</button>
                        <button type="button" class="btn btn-default" ng-class="{'active': css.currentReportMenu=='C5'}" ng-click="clickChartMenu('C5')">{{'ReportMenuMarketTrends' | translate}}</button>
                        <button type="button" class="btn btn-default" ng-class="{'active': css.currentReportMenu=='C6'}" ng-click="clickChartMenu('C6')">{{'ReportMenuMarketIndicator' | translate}}</button>
                    </div>

                    <br />
                    {{'FinalScoreAdminPanelLabel' | translate}} :
                    <button type="button" class="btn btn-default btn-sm" ng-click="clickChartMenu('AllDecisions')"> {{'FinalScoreAdminPanelDecisionsButton' | translate}}</button>
                    <button type="button" class="btn btn-default btn-sm" ng-click="clickChartMenu('AllResults')"> {{'FinalScoreAdminPanelResultsButton' | translate}}</button>
                    <button type="button" class="btn btn-default btn-sm" ng-click="clickChartMenu('Questionnaire')"> {{'QuestionnairePageQuestionnaire' | translate}}</button>
                </div>


                <div class="col-md-5" ng-if="css.currentReportMenu=='AllDecisions'">
                    <form class="form-inline" role="form" >
                        <div class="form-group ">
                            <button type="submit" class="btn btn-default btn-sm" ng-click="reRunSeminar()">{{'FinalScoreAdminPanelReRunButton' | translate}}</button>

                            <label class="checkbox-inline" ng-repeat="companyid in data.allDecisions.allCompanyId">
                                <input type="checkbox" ng-model="data.reRunCompanies[$index]"> {{'HomePageSecondMenuBarLabelsCompany' | translate}} {{companyid | companyname}}
                            </label>
                        </div>
                    </form>

                    <br />

                    <form class="form-inline" role="form" name="skuForm" >
                        <div class="form-group" ng-class="{'has-success':skuForm.$valid&&skuForm.SKUField.$dirty, 'has-error':skuForm.$invalid&&skuForm.SKUField.$dirty}">
                            <label class="sr-only" for="SKUField">{{data.reRunDecision.sku_fieldname}}</label>
                            <label class="control-label" for="SKUField" ng-show="skuForm.SKUField.$error.required&&skuForm.$dirty"><i class="fa fa-times-circle-o"></i> Required !</label>
                            <input type="text" class="form-control input-sm" id="SKUField" name="SKUField" placeholder="{{data.reRunDecision.sku_fieldname}}" ng-model="data.reRunDecision.sku_fieldvalue" required>
                        </div>

                        <button type="submit" class="btn btn-default btn-sm" ng-click="updateSkuDecision()" ng-disabled="skuForm.$invalid">
                            {{'FinalScoreAdminPanelReRunSaveButton' | translate}}  {{'HomePageSecondMenuBarCurrentPeriod' | translate}}{{data.reRunDecision.periodId}} {{'HomePageSecondMenuBarLabelsCompany' | translate}}{{data.reRunDecision.companyId | companyname}} {{data.reRunDecision.type}} {{data.reRunDecision.sku_fieldname}}
                        </button>
                    </form>

                    <br/>

                    <form class="form-inline" role="form" >
                        <div class="form-group ">
                            <button type="submit" class="btn btn-default btn-sm" ng-click="unlockDecisions()">{{'FinalScoreAdminPanelUnlockDecisionButton' | translate}}</button>

                            <label class="checkbox-inline" ng-repeat="companyid in data.allDecisions.allCompanyId">
                                <input type="checkbox" ng-model="data.unlockCompanyDecisions[$index]"> {{'HomePageSecondMenuBarLabelsCompany' | translate}} {{companyid | companyname}}
                            </label>
                        </div>
                    </form>
                </div>
            </div>

        </section>



        <!--  Table A1  -->
        <section class="content sectionbox" ng-if="css.currentReportMenu=='A1'">
            <!-- Report box -->
            <div class="box box-primary">
                <div class="box-header">
                    <div class="pull-right box-tools">
                        <div class="btn-group">
                            <button type="button" class="btn btn-default btn-sm dropdown-toggle" data-toggle="dropdown">
                                {{'HomePageSecondMenuBarLabelsCompany' | translate}} {{data.tableA1CompanyStatus.currentCompany.companyname}} <span class="caret"></span>
                            </button>
                            <ul class="dropdown-menu" role="menu">
                                <li ng-repeat="company in data.tableA1CompanyStatus.allCompanyData"><a href="" ng-click="switchTableReportA1Company(company)">{{'HomePageSecondMenuBarLabelsCompany' | translate}} {{company.companyName}}</a></li>
                            </ul>
                        </div>

                        <div class="btn-group btn-group-sm">
                            <button type="button" class="btn btn-default" ng-class="{'active': css.tableReportTab=='SKU'}" ng-click="css.tableReportTab='SKU' ">{{'ReportTabLabelSKU' | translate}}</button>
                            <button type="button" class="btn btn-default" ng-class="{'active': css.tableReportTab=='Brand'}" ng-click="css.tableReportTab='Brand' ">{{'ReportTabLabelBrand' | translate}}</button>
                            <button type="button" class="btn btn-default" ng-class="{'active': css.tableReportTab=='Global'}" ng-click="css.tableReportTab='Global' ">{{'ReportTabLabelGlobal' | translate}}</button>
                        </div>
                    </div>

                    <i class="fa fa-table"></i>
                    <h3 class="box-title">
                        {{'ReportMenuCompanyStatus' | translate}}
                    </h3>
                </div>

                <div class="box-body no-padding">
                    <!-- .table - Uses sparkline charts-->
                    <div ng-if="css.tableReportTab=='SKU'">
                        <ul class="nav nav-pills">
                            <li ng-class="{'active': data.tableA1CompanyStatus.currentSKU.SKUID==sku.SKUID}" ng-repeat="sku in data.tableA1CompanyStatus.currentCompany.SKU"><a href="" ng-click="switchTableReportA1SKU(sku)">{{sku.SKUName}} {{sku.PackSize | skupacksize | translate}}</a></li>
                        </ul>
                        <div class="divider_horizontal"></div>
                        <div table-report-company-status-sku data="data.tableA1CompanyStatus.currentSKU.data"></div>
                    </div>
                    <div ng-if="css.tableReportTab=='Brand'">
                        <ul class="nav nav-pills">
                            <li ng-class="{'active': data.tableA1CompanyStatus.currentBrand.brandId==brand.brandId}" ng-repeat="brand in data.tableA1CompanyStatus.currentCompany.brand"><a href="" ng-click="switchTableReportA1Brand(brand)">{{brand.brandName}}</a></li>
                        </ul>
                        <div class="divider_horizontal"></div>
                        <div table-report-company-status-brand data="data.tableA1CompanyStatus.currentBrand.data"></div>
                    </div>
                    <div ng-if="css.tableReportTab=='Global'">
                        <div table-report-company-status-global data="data.tableA1CompanyStatus.currentGlobal"></div>
                    </div>
                </div><!-- /.box-body-->

            </div>
            <!-- /.box -->
        </section>



        <!--  Table A2  -->
        <section class="content sectionbox" ng-if="css.currentReportMenu=='A2'">
            <div class="box box-primary">
                <div class="box-header">
                    <div class="pull-right box-tools">
                        <div class="btn-group">
                            <button type="button" class="btn btn-default btn-sm dropdown-toggle" data-toggle="dropdown">
                                {{'HomePageSecondMenuBarLabelsCompany' | translate}} {{data.tableA2FinancialData.currentCompany.companyName}} <span class="caret"></span>
                            </button>
                            <ul class="dropdown-menu" role="menu">
                                <li ng-repeat="data in data.tableA2FinancialData.allData"><a href="" ng-click="switchTableReportA2Company($index)">{{'HomePageSecondMenuBarLabelsCompany' | translate}} {{data.companyName}}</a></li>
                            </ul>
                        </div>
                        <div class="btn-group">
                            <button type="button" class="btn btn-default btn-sm dropdown-toggle" data-toggle="dropdown">
                                {{'HomePageSecondMenuBarCurrentPeriod' | translate}} {{data.tableA2FinancialData.currentPeriod.period}} <span class="caret"></span>
                            </button>
                            <ul class="dropdown-menu" role="menu">
                                <li ng-repeat="period in data.tableA2FinancialData.currentCompany.periods"><a href="" ng-click="switchTableReportPeriod(period)">{{'HomePageSecondMenuBarCurrentPeriod' | translate}} {{period.period}}</a></li>
                            </ul>
                        </div>
                    </div>

                    <i class="fa fa-table"></i>
                    <h3 class="box-title">
                        {{'ReportMenuFinancialReport' | translate}}
                    </h3>
                </div>
                <div class="box-body no-padding">

                    <ul class="nav nav-pills">
                        <li ng-class="{'active': data.tableA2FinancialData.currentBrand.brandId==brand.brandId}" ng-repeat="brand in data.tableA2FinancialData.currentPeriod.brands"><a href="" ng-click="switchTableReportA2Brand(brand)">{{brand.brandName}}</a></li>
                        <li ng-class="{'active': data.tableA2FinancialData.currentBrand=='allbrand'}"><a href="" ng-click="switchTableReportA2Brand('allbrand')">{{data.tableA2FinancialData.currentPeriod.allBrands.brandName}}</a></li>
                    </ul>

                    <div class="divider_horizontal"></div>
                    <div table-report-financial-report-brand data="data.tableA2FinancialData.currentBrand.data" ng-if="data.tableA2FinancialData.currentBrand!=='allbrand'"></div>
                    <div table-report-financial-report-all-brand data="data.tableA2FinancialData.currentPeriod.allBrands.data" ng-if="data.tableA2FinancialData.currentBrand=='allbrand'"></div>

                </div>

            </div>
        </section>



        <!--  Chart A3  -->
        <section class="content sectionbox" ng-if="css.currentReportMenu=='A3'">
            <div class="box box-primary ">
                <div class="box-header">
                    <div class="pull-right box-tools">
                        <div class="btn-group">
                            <button type="button" class="btn btn-default btn-sm dropdown-toggle" data-toggle="dropdown">
                                {{'HomePageSecondMenuBarLabelsCompany' | translate}} {{data.chartA3InventoryReport.currentCompany.companyName}} <span class="caret"></span>
                            </button>
                            <ul class="dropdown-menu" role="menu">
                                <li ng-repeat="company in data.chartA3InventoryReport.allData"><a href="" ng-click="switchChartA3Company($index)">{{'HomePageSecondMenuBarLabelsCompany' | translate}} {{company.companyName}}</a></li>
                            </ul>
                        </div>
                    </div>

                    <i class="fa fa-bar-chart-o"></i>
                    <h3 class="box-title">
                        {{'ReportMenuInventoryReport' | translate}}
                    </h3>
                </div>
                <div class="box-body no-padding">
                    <nvd3-multi-bar-chart data="data.chartA3InventoryReport.data" tooltipcontent="A3ToolTipContent()" color="A3ColorFunction()" legendcolor="A3ColorFunction()"
                          height="600" xaxislabel="{{'ReportInventoryReportTableLabelX' | translate}}" yaxislabel="{{'ReportInventoryReportTableLabelY' | translate}}" margin="{left:100}"
                          showXAxis="true" showYAxis="true" stacked="true" showLegend="true" reduceXTicks="false" tooltips="true" interactive="true">
                        <svg class="IE11_chart_A3"></svg>
                    </nvd3-multi-bar-chart>
                </div>
            </div>
        </section>



        <!--  Table A4  -->
        <section class="content sectionbox" ng-if="css.currentReportMenu=='A4'">
            <div class="box box-primary">
                <div class="box-header">
                    <div class="pull-right box-tools">
                        <div class="btn-group">
                            <button type="button" class="btn btn-default btn-sm dropdown-toggle" data-toggle="dropdown">
                                {{'HomePageSecondMenuBarLabelsCompany' | translate}} {{data.tableA4ProfitabilityEvolution.currentData.companyName}} <span class="caret"></span>
                            </button>
                            <ul class="dropdown-menu" role="menu">
                                <li ng-repeat="data in data.tableA4ProfitabilityEvolution.allData"><a href="" ng-click="switchTableReportA4Company($index)">{{'HomePageSecondMenuBarLabelsCompany' | translate}} {{data.companyName}}</a></li>
                            </ul>
                        </div>

                        <div class="btn-group btn-group-sm">
                            <button type="button" class="btn btn-default" ng-class="{'active': css.tableReportTab=='SKU'}" ng-click="css.tableReportTab='SKU' ">{{'ReportTabLabelSKU' | translate}}</button>
                            <button type="button" class="btn btn-default" ng-class="{'active': css.tableReportTab=='Brand'}" ng-click="css.tableReportTab='Brand' ">{{'ReportTabLabelBrand' | translate}}</button>
                            <button type="button" class="btn btn-default" ng-class="{'active': css.tableReportTab=='Global'}" ng-click="css.tableReportTab='Global' ">{{'ReportTabLabelGlobal' | translate}}</button>
                        </div>
                    </div>
                    <i class="fa fa-table"></i>
                    <h3 class="box-title">
                        {{'ReportMenuProfitabilityEvolution' | translate}}
                    </h3>
                </div>
                <div class="box-body no-padding">

                    <div ng-if="css.tableReportTab=='SKU'">

                        <ul class="nav nav-pills">
                            <li ng-class="{'active': data.tableA4ProfitabilityEvolution.currentSKU.SKUID==sku.SKUID}" ng-repeat="sku in data.tableA4ProfitabilityEvolution.currentData.SKU"><a href="" ng-click="switchTableReportA4SKU(sku)">{{sku.SKUName}} {{sku.PackSize | skupacksize | translate}}</a></li>
                        </ul>

                        <div class="divider_horizontal"></div>
                        <div table-report-profitability-evolution-sku data="data.tableA4ProfitabilityEvolution.currentSKU.data"></div>

                    </div>

                    <div ng-if="css.tableReportTab=='Brand'">

                        <ul class="nav nav-pills">
                            <li ng-class="{'active': data.tableA4ProfitabilityEvolution.currentBrand.brandId==brand.brandId}" ng-repeat="brand in data.tableA4ProfitabilityEvolution.currentData.brand"><a href="" ng-click="switchTableReportA4Brand(brand)">{{brand.brandName}}</a></li>
                        </ul>

                        <div class="divider_horizontal"></div>
                        <div table-report-profitability-evolution-brand data="data.tableA4ProfitabilityEvolution.currentBrand.data"></div>

                    </div>

                    <div ng-if="css.tableReportTab=='Global'">
                        <div table-report-profitability-evolution-global data="data.tableA4ProfitabilityEvolution.currentGlobal"></div>
                    </div>

                </div>
            </div>
        </section>



        <!--  Table B1  -->
        <section class="content sectionbox" ng-if="css.currentReportMenu=='B1'">

            <!--  row 1  -->
            <div class="col-md-6">
                <div class="box box-primary">
                    <div class="box-header">
                        <i class="fa fa-bar-chart-o"></i>
                        <h3 class="box-title">
                            {{'ReportMarketShareChartTitleMarketShareInValue' | translate}}
                        </h3>
                    </div>
                    <div class="box-body no-padding">
                        <div ac-chart="'line'" ac-data="data.chartB11MarketShareInValue.data" ac-config="data.chartB11MarketShareInValue.config" class="chartBox"></div>
                        <p class="text-center">{{'ReportMarketSalesAndInventoryChartBottomTextPeriod' | translate}}</p>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="box box-primary">
                    <div class="box-header">
                        <i class="fa fa-bar-chart-o"></i>
                        <h3 class="box-title">
                            {{'ReportMarketShareChartTitleMarketShareInVolume' | translate}}
                        </h3>
                    </div>
                    <div class="box-body no-padding">
                        <div ac-chart="'line'" ac-data="data.chartB12MarketShareInVolume.data" ac-config="data.chartB12MarketShareInVolume.config" class="chartBox"></div>
                        <p class="text-center">{{'ReportMarketSalesAndInventoryChartBottomTextPeriod' | translate}}</p>
                    </div>
                </div>
            </div>
            <!--  \row 1  -->
            <!-- row 2  -->
            <div class="col-md-6">
                <div class="box box-primary">
                    <div class="box-header">
                        <i class="fa fa-bar-chart-o"></i>
                        <h3 class="box-title">
                            {{'ReportMarketShareChartTitleMindSpaceShare' | translate}}
                        </h3>
                    </div>
                    <div class="box-body no-padding">
                        <div ac-chart="'line'" ac-data="data.chartB13MindSpaceShare.data" ac-config="data.chartB13MindSpaceShare.config" class="chartBox"></div>
                        <p class="text-center">{{'ReportMarketSalesAndInventoryChartBottomTextPeriod' | translate}}</p>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="box box-primary">
                    <div class="box-header">
                        <i class="fa fa-bar-chart-o"></i>
                        <h3 class="box-title">
                            {{'ReportMarketShareChartTitleShelfSpaceShare' | translate}}
                        </h3>
                    </div>
                    <div class="box-body no-padding">
                        <div ac-chart="'line'" ac-data="data.chartB14ShelfSpaceShare.data" ac-config="data.chartB14ShelfSpaceShare.config" class="chartBox"></div>
                        <p class="text-center">{{'ReportMarketSalesAndInventoryChartBottomTextPeriod' | translate}}</p>
                    </div>
                </div>
            </div>
            <!--  \row 2  -->
        </section>



        <!--  Table B2  -->
        <section class="content sectionbox" ng-if="css.currentReportMenu=='B2'">
            <div class="box box-primary">
                <div class="box-header">
                    <i class="fa fa-table"></i>
                    <h3 class="box-title">{{'ReportMenuCompetitorIntelligence' | translate}}</h3>
                </div>
                <div class="box-body no-padding">
                    <ul class="nav nav-pills table_multimenu">
                        <li ng-class="{'active': data.tableB2CompetitorIntelligence.tableReportMenu == 1}"><a href="" ng-click="switchTableMenuLevel1B2(1, 'acquiredProductionAndLogisticsEfficiency', '%')">{{'ReportCompetitorIntelligenceTableTitleTechnology' | translate}}</a></li>
                        <li ng-class="{'active': data.tableB2CompetitorIntelligence.tableReportMenu == 2}"><a href="" ng-click="switchTableMenuLevel1B2(2, 'additionalTradeMarginCost', '')">{{'ReportCompetitorIntelligenceTableTitleMarketingSales' | translate}}</a></li>
                        <li ng-class="{'active': data.tableB2CompetitorIntelligence.tableReportMenu == 3}"><a href="" ng-click="switchTableMenuLevel1B2(3, 'capacityUtilisationRate', '%')">{{'ReportCompetitorIntelligenceTableTitleOperations' | translate}}</a></li>
                        <li ng-class="{'active': data.tableB2CompetitorIntelligence.tableReportMenu == 4}"><a href="" ng-click="switchTableMenuLevel1B2(4, 'investmentToImproveTechnologyLevel', '')">{{'ReportCompetitorIntelligenceTableTitleInvestments' | translate}}</a></li>
                    </ul>
                    <ul class="nav nav-pills" ng-if="data.tableB2CompetitorIntelligence.tableReportMenu == 1">
                        <li ng-class="{'active': data.tableB2CompetitorIntelligence.currentTable==1}"><a href="" ng-click="switchTableReportB2(1, 'acquiredProductionAndLogisticsEfficiency', '%')">{{'ReportCompetitorIntelligenceTechnologyAcquiredProductionAndLogisticsEfficiency' | translate}}</a></li>
                        <li ng-class="{'active': data.tableB2CompetitorIntelligence.currentTable==2}"><a href="" ng-click="switchTableReportB2(2, 'acquiredProductionPlanningFlexibility', '%')">{{'ReportCompetitorIntelligenceTechnologyAcquiredProductionPlanningFlexibility' | translate}}</a></li>
                        <li ng-class="{'active': data.tableB2CompetitorIntelligence.currentTable==3}"><a href="" ng-click="switchTableReportB2(3, 'availableTechnologyLevel', '')">{{'ReportCompetitorIntelligenceTechnologyAvailableTechnologyLevel' | translate}}</a></li>
                    </ul>

                    <ul class="nav nav-pills" ng-if="data.tableB2CompetitorIntelligence.tableReportMenu == 2">
                        <li ng-class="{'active': data.tableB2CompetitorIntelligence.currentTable==1}"><a href="" ng-click="switchTableReportB2(1, 'additionalTradeMarginCost', '')">{{'ReportCompetitorIntelligenceMarketingSalesAdditionalTradeMarginCost' | translate}} </a></li>
                        <li ng-class="{'active': data.tableB2CompetitorIntelligence.currentTable==2}"><a href="" ng-click="switchTableReportB2(2, 'advertising', '')">{{'ReportCompetitorIntelligenceMarketingSalesAdvertising' | translate}}</a></li>
                        <li ng-class="{'active': data.tableB2CompetitorIntelligence.currentTable==3}"><a href="" ng-click="switchTableReportB2(3, 'consumerPromotionsCost', '')">{{'ReportCompetitorIntelligenceMarketingSalesConsumerPromotionsCost' | translate}}</a></li>
                        <li ng-class="{'active': data.tableB2CompetitorIntelligence.currentTable==4}"><a href="" ng-click="switchTableReportB2(4, 'retailersPurchasesVolumeStdPack', '')">{{'ReportCompetitorIntelligenceMarketingSalesRetailerPurchaseVolume' | translate}}</a></li>
                        <li ng-class="{'active': data.tableB2CompetitorIntelligence.currentTable==5}"><a href="" ng-click="switchTableReportB2(5, 'salesForceCost', '')">{{'ReportCompetitorIntelligenceMarketingSalesSalesForceCost' | translate}}</a></li>
                        <li ng-class="{'active': data.tableB2CompetitorIntelligence.currentTable==6}"><a href="" ng-click="switchTableReportB2(6, 'shipmentsToWholesalers', '')">{{'ReportCompetitorIntelligenceMarketingSalesShipmentToWholesalers' | translate}}</a></li>
                        <li ng-class="{'active': data.tableB2CompetitorIntelligence.currentTable==7}"><a href="" ng-click="switchTableReportB2(7, 'tradeInvestment', '')">{{'ReportCompetitorIntelligenceMarketingSalesTradeInvestments' | translate}}</a></li>
                        <li ng-class="{'active': data.tableB2CompetitorIntelligence.currentTable==8}"><a href="" ng-click="switchTableReportB2(8, 'volumeDiscountCost', '')">{{'ReportCompetitorIntelligenceMarketingSalesVolumeDiscountCost' | translate}}</a></li>
                    </ul>

                    <ul class="nav nav-pills" ng-if="data.tableB2CompetitorIntelligence.tableReportMenu == 3">
                        <li ng-class="{'active': data.tableB2CompetitorIntelligence.currentTable==1}"><a href="" ng-click="switchTableReportB2(1, 'capacityUtilisationRate', '%')">{{'ReportCompetitorIntelligenceOperationsCapacityUtilisationRate' | translate}}  </a></li>
                        <li ng-class="{'active': data.tableB2CompetitorIntelligence.currentTable==2}"><a href="" ng-click="switchTableReportB2(2, 'inventoryVolumeAtManufacturerStdPack', '')">{{'ReportCompetitorIntelligenceOperationsInventoryVolumeAtManufacturer' | translate}}  </a></li>
